<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kgitbank.jeju.mapper.matzipXMLMapper">
	
	<select id="getList" resultType="com.kgitbank.jeju.dto.matzipDto">
		SELECT * FROM matzip
	</select>
	
	<select id="getSpot" resultType="com.kgitbank.jeju.dto.matzipDto">
		SELECT * FROM matzip WHERE title = #{title} 
	</select>
	
</mapper>